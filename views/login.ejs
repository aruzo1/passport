<h1>Login</h1>
<% if (messages.error){ %>
    <%= messages.error %>
<% } %>
<form method="POST" action="login">
    <div>
        email
        <input type="email" name="email" required/>
    </div>
    <div>
        Password
        <input type="password" name="password" required/>
    </div>
    <div>
        <button type="submit">Login</button>
    </div>
</form>
<a href="/register">Register</a>